str=input()
u=0
d=0
a=0
l=0
for i in str:
    if(i.isdigit()):
      d=d+1
    elif(i.isupper()):
       u=u+1
    elif(i.islower()):
        l=l+1
    else:
       a=u+l
print('digit',d)
print('alphabets',a)
print('uppercase',u)
print('lowercase',l)
